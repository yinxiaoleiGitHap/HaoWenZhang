<% include ./header %>
   
<table class="table table-hover">
    <tr>
        <td>排序</td>
        <td>类别名称</td>
        <td>类别介绍</td>
        <td>创建时间</td>
        <td>链接地址</td>
        <td>操作</td>
    </tr>
    <% datalist.forEach(function(val){ %>
        <tr>
            <td><%=val.order%></td>
            <td><%=val.name%></td>
            <td><%=val.description%></td>
            <td><%=val.ctime.toLocaleString()%></td>
            <td><%=val.dizhi%></td>
            <td>
                <a href="/admin/loveEdit/<%=val._id%>">编辑</a>
                |
                <a href="/admin/loveRemove/<%=val._id%>" onclick="return confirm('您确定要删除吗？')">删除</a>
            </td>
        </tr>
    <% })%>
</table>

    
<% include ./footer %>